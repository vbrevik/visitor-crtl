    "realm": [
      { "name": "sponsor", "description": "Can sponsor visitors for site access", "composite": false },
      { "name": "reception_guard", "description": "Reception desk guard role", "composite": false },
      { "name": "security_officer", "description": "Security officer with elevated privileges", "composite": false },
      { "name": "escort", "description": "Can escort visitors on site", "composite": false },
      { "name": "unit_manager", "description": "Unit manager with approval authority", "composite": false },
      { "name": "site_admin", "description": "Site administrator with full access", "composite": false },
      { "name": "auditor", "description": "Read-only audit access", "composite": false },
      { "name": "default-roles-mil-feide", "description": "Default roles for mil-feide realm", "composite": false }
    ]
  },
